<html>
    <head>

    </head>
    <body>
        <div class="container-fluid" style="margin-top: 150px;vertical-align: middle">
            <div class="row" style="background-color:#364150;border-color: aliceblue;border-style: solid;border-width: 2px;border-radius: 15px 50px;padding: 10px">
                <div class="navbar" style="height: 20px;color: aliceblue;font-size: larger;padding: 10px"><center><b style="">DOCTOR DIRECTORY</b></center></div>
            </div>
            <div class="row" style="padding:100px;">
                <div class="col-sm-4" style="padding-bottom:10px;" ng-repeat="doctor in doctors">
                    <img class="img-responsive img-circle" style="height:400px;width:400px;"ng-src="../img/doctor-{{doctor.gender}}-{{doctor.num}}.jpg"><br>
                    <div style="text-align: center; font-weight: bold;">{{doctor.name}}</div>
                    <div style="text-align: center; font-weight: bold;">{{doctor.speciality}}</div>
                </div>
            </div>
        </div>
    </body>
</html>